<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.2//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">

<validators>
	<field name="manipel.courseOfStudy">
		<field-validator type="requiredstring">
			<message key="msg.validator.required" />
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">100</param>
			<param name="minLength">2</param>
			<message key="msg.validator.stringLength" />
		</field-validator>
	</field>
	<field name="manipel.vintage">
		<field-validator type="required">
			<message key="msg.validator.required" />
		</field-validator>
		<field-validator type="int">
			<param name="min">2011</param>
			<param name="max">2999</param>
			<message key="msg.validator.intRange" />
		</field-validator>
	</field>

</validators>